prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@

Name: gpu-delaunay-generator
Description: A GPU implemented Delaunay triangular generator for C++ using CUDA
Version: @YAML_CPP_VERSION@
Requires:
Libs: -L${libdir} -lgpu-delaunay-generator
Cflags: -I${includedir}